<Window x:Class="Tying_test.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tying_test"
        mc:Ignorable="d"
        Title="Typing test" Height="450" Width="800">
    <Grid RenderTransformOrigin="0.648,0.666">
        <TextBox x:Name="textboxForUserToDropTextIn" HorizontalAlignment="Center" Margin="0,269,0,0" TextWrapping="Wrap" Text="Hello" VerticalAlignment="Top" Width="120" TextChanged="TextBox_TextChanged_1" />
        <Button x:Name="btnGenerateTypingTest" Content="Send text" HorizontalAlignment="Left" Margin="566,287,0,0" VerticalAlignment="Top" Click="SendText" />
        <TextBox x:Name="typingTest" Visibility="Collapsed" HorizontalAlignment="Center" TextWrapping="Wrap" Text="Hello" VerticalAlignment="Center" Width="120" TextChanged="TextBox_TextChanged_1" RenderTransformOrigin="0.467,-0.944" />
    </Grid>
</Window>
